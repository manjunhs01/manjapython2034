      "source": [
        "number = int(input(\"Enter a number:\"))\n",
        "reverse_number = 0\n",
        "temp = number\n",
        "\n",
        "while temp > 0:\n",
        "  digit = temp % 10\n",
        "  reverse_number = reverse_number * 10 + digit\n",
        "  temp //= 10\n",
        "\n",
        "if number == reverse_number:\n",
        "  print(f\"{number} is a polindrome\")\n",
        "else:\n",
        "  print(f\"{number} is not a polindrome\")"
      ]
    }
  ]
}